/*login*/
.login_wrap {width:100%; min-height:918px; background:url('../img/bg_login.png') no-repeat; position:relative;}
.login_wrap .login_area {position:absolute; width:500px; height:320px; top:50%; left:50%; margin:-160px 0 0 -250px;}
.login_wrap .login_area .title {font-size:32px; font-weight:700; color:#fff; text-align:center;}
.login_wrap .login_area .login {width:500px; border-radius:17px; background-color:#5974d7;padding:40px 24px;box-sizing:border-box; box-shadow:0px -1px 7px 3px #405EBB}
.login_wrap .login_area .login > p{padding-left:115px; position:relative;}
.login_wrap .login_area .login > p .sub_title {position:absolute; width:100px; text-align:right; top:10px; left:0; font-size:16px; font-weight:700; color:#fff;}









