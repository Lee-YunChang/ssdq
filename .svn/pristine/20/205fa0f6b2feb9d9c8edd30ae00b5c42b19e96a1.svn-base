/* main */
.container-main{margin-top:-1.5rem; }
.container-main .main_spot{background:url("/img/bg_main.png") no-repeat 610px top #bed4fc;background-size: contain; padding:50px; -webkit-box-shadow: inset 0px 6px 12px -6px rgba(0,0,0,0.25); -moz-box-shadow: inset 0px 6px 12px -6px rgba(0,0,0,0.25); box-shadow: inset 0px 6px 12px -6px rgba(0,0,0,0.25);}
.container-main .main_spot h2{font-size:38px; font-weight:600; color:#2653d4; }
