    @media screen and (-ms-high-contrast: active), (-ms-high-contrast: none){
        .quick_menu_area ul li .title {letter-spacing:-1.3px;}
    .quick_menu_area ul li .txt {letter-spacing:-1.8px;}
    }


/*write_db_area*/
.write_db_area {width:100%; min-height:950px; background-color:#4260cf}
