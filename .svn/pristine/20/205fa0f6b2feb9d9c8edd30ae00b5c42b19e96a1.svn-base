.table_type100 {
	color:#858796 !important; 
	text-align:center;
	font-size: 13px;
	font-family: "맑은 고딕", "Malgun Gothic", Dotum, Arial, sans-serif;
	background-color:#fff; 
}
.table-striped tbody tr:nth-of-type(odd) {background-color:#ffffff;}
.table-striped tbody tr:nth-of-type(even) {background-color:#ffffff;}
.table_type100 table th {
	cursor : pointer;
	text-align: center;
   	font-weight: bold;
	border: 1px solid #E3E6F0;
	background-color: #F8F9FC;
	vertical-align: middle;
}
.table_type100 table td {
	cursor : pointer;
	vertical-align: middle;
}
.DTFC_LeftWrapper div table {border-right: 1px solid #AAAAAA;}
.dataTables_scrollHeadInner table thead tr th {border-bottom: 2px solid #E3E6F0;}
.DTFC_LeftHeadWrapper table thead tr th {border-bottom: 2px solid #E3E6F0;}





