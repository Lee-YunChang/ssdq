.btn_type01 {width:80px; height:34px; line-height:34px; text-align:center; border-radius:5px; border:0; background-color:#2b3151; font-size:14px; color:#fff;}
.btn_type02 {width:120px; height:40px; line-height:40px; text-align:center; border-radius:25px; border:0; background-color:#3958ca; color:#fff; font-size:16px; font-weight:700;}
