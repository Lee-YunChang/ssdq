.container-main .main_spot p{color:#5a5c69; font-size:12px;}
.container-main .main_spot a{display:inline-block; color:#4262be; font-size:15px; border:2px solid #4262be; border-radius:3px; font-weight:600; line-height:2; padding:0 25px;}
.container-main .main_card {padding-left:25px; padding-top:25px; width:1200px; margin-bottom:70px; margin:0 auto;}
.container-main .main_card ul{overflow:hidden; padding:0;}
.container-main .main_card ul li{float:left; margin-right:10px; border:1px solid #e3e6f0; border-radius:4px; padding:20px;text-align:center; background-color:#fff; width:215px; webkit-box-shadow: 0px 0px 4px -2px rgba(194,194,194,1); -moz-box-shadow: 0px 0px 4px -2px rgba(194,194,194,1); box-shadow: 0px 0px 4px -2px rgba(194,194,194,1); word-break: keep-all; }
.container-main .main_card ul li img{display:inline-block; margin-top:40px;}
.container-main .main_card ul li em{display:block; margin:30px 0 10px 0; color:#333333; font-size:18px; font-style:normal; font-weight:600;}
.container-main .main_card ul li p{font-size:14px; color:#858796; height:90px; line-height:1.4;}
.container-main .main_card ul li a{display:inline-block; width:65%; border-radius:4px; padding:6px;color:#fff; font-size:14px; background-color:#4e73df; font-weight:600;}



/*quick_menu_area*/
.quick_menu_area ul {padding:0;}
.quick_menu_area ul li {position:relative;width:175px; height:350px; padding:117px 14px 20px 14px; box-sizing:border-box; border:1px solid #dcdfe8; border-radius:3px; background-color:#fff; float:left; margin-left:10px; list-style:none;}
