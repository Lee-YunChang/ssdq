<script type="text/javascript">
$(document).ready(function() {

});

function doSave() {
	
	// 업로드 파일 확인
	if($("#fileName").val() == "") {
		alert("업로드할 파일을 선택해주세요");
		return;
	}
	
	if(confirm("저장하시겠습니까?")) {
// 		var uploadDt = $("#uploadDt").val().replace(/\-/g,'');
// 		$("#uploadDt").val(uploadDt);
		$("#frm").submit();
	}
	
}

</script>
	<form name="frm" id="frm"  method="post" action="/test/ExcelUpProc.do" enctype="multipart/form-data">
           	<div class="table_wrap">
				<table class="col_type">
             		<colgroup>
                        <col style="width: 15%" />
                        <col style="width: 35%" />
                        <col style="width: 20%" />
                        <col style="width: 30%" />
                    </colgroup>
				 	<thead>
				 		<tr>
							<th>파일찾기</th>
	                        <td colspan="3">&nbsp;&nbsp;&nbsp;<input type="file" name="fileName" id="fileName" /></td>
				 		</tr>
                 	</thead>
                 </table>
            </div>
        </form>					

	<div class="pop_btn_area">
		<div class="btn_right">
			<button type="button" class="btn blue" onclick="doSave(); return false;">업로드</button>
			<button type="button" class="btn gray" onclick="self.close();">닫기</button>
		</div>
	</div>
</body>
