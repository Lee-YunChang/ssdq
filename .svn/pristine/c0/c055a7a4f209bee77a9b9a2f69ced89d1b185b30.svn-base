<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">


	<nav th:fragment="headerFragment" class="navbar">
	  <div class="navbar_left">
        <a class="btn_fold" title="메뉴접기"></a>
        <span class="link_txt" th:if="${conType != null}" th:text="${#strings.contains(conType, 'CSV')} ? '#{UIT0054} : ' + ${session.conFileName } : '#{UIT0055} : ' + ${session.conDbUrl }"></span>
        <div class="navtitle_box">
             <span th:if="${conType != null}" th:text="${#strings.contains(conType, 'CSV')} ? '#{UIT0054} : ' + ${session.conFileName } : '#{UIT0055} : ' + ${session.conDbUrl }"></span>
        </div>
        </div>
        <div class="navbar_right">
            <span class="account_date">2020.02.12</span>
            <div class="account_info" th:each="member : ${session.member}">
                <span th:text="${member.userNm}"></span>
            </div>
            
            <a class="logout_btn" href="#" data-toggle="modal" data-target="#logoutModal" title="로그아웃" >
                <span>로그아웃</span>
                <span class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-gray-600" th:text="#{UIT0051}"> 로그아웃</span>
            </a>
        </div>
	</nav>

</html>