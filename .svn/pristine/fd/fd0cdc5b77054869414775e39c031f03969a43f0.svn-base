	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.1.4.RELEASE</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.webapp</groupId>
	<artifactId>ROOT</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<!-- <version>1.2.5-SNAPSHOT</version> -->
	<packaging>jar</packaging>
	
	<profiles>
	
		<profile>
	    	<id>local</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<env>local</env>
			</properties>
		</profile>
		
		<profile>
			<id>dev</id>
			<properties>
				<env>dev</env>
			</properties>
		</profile>
		
		<profile>
			<id>desktop</id>
			<properties>
				<env>desktop</env>
			</properties>
		</profile>
		
	</profiles>
	
	<build>
		<finalName>ROOT</finalName>
		<defaultGoal>install</defaultGoal>
		<directory>${basedir}/target</directory>
		
		<resources>
			<resource>
				<directory>src/main/webapp</directory>
			</resource>
			<resource>
                <directory>src/main/resources</directory>
            </resource>
			<resource>
				<directory>src/main/resources-${env}</directory>
			</resource>
	    </resources>
        
			<plugins>
				<plugin>
					<groupId>org.springframework.boot</groupId>
					<artifactId>spring-boot-maven-plugin</artifactId>
					
					<configuration>
						<warSourceDirectory>src/main/webapp</warSourceDirectory>
						<webXml>src/main/webapp/WEB-INF/web.xml</webXml>
						<executable>true</executable>
	               	</configuration>
				</plugin>
				
				<plugin>
		   			<artifactId>maven-antrun-plugin</artifactId>
		  				<executions> 
		  					<execution>
		  						<id>exec01</id>
		  						<phase>compile</phase>
		  						<goals>
				                   <goal>run</goal>
				                </goals>
		  						<configuration>
		  							<tasks>
		   							<copy file="src/main/resources/resources-${env}/application.properties" overwrite="true" tofile="${project.basedir}/target/${artifactId}/WEB-INF/classes/application.properties"/>
									<delete dir="${project.build.outputDirectory}/resources-dev" includeemptydirs="true"/>
									<delete dir="${project.build.outputDirectory}/resources-local" includeemptydirs="true"/>		   							
		  							</tasks>
		  						</configuration>
		  					</execution>
					</executions>
		  		</plugin>
				
			</plugins>


			<pluginManagement>
				<plugins>
					<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
					<plugin>
						<groupId>org.eclipse.m2e</groupId>
						<artifactId>lifecycle-mapping</artifactId>
						<version>1.0.0</version>
						<configuration>
							<lifecycleMappingMetadata>
								<pluginExecutions>
									<pluginExecution>
										<pluginExecutionFilter>
											<groupId>
												org.apache.maven.plugins
											</groupId>
											<artifactId>
												maven-antrun-plugin
											</artifactId>
											<versionRange>
												[1.8,)
											</versionRange>
											<goals>
												<goal>run</goal>
											</goals>
										</pluginExecutionFilter>
										<action>
											<ignore></ignore>
										</action>
									</pluginExecution>
								</pluginExecutions>
							</lifecycleMappingMetadata>
						</configuration>
					</plugin>
				</plugins>
			</pluginManagement>
	</build>
	

