@charset "utf-8";


/* 폰트, color는 디자인가이드에 맞게사용 */
body {font-size: 14px;}
input, button, select, textarea {font-size: 12px; margin: 0px; padding: 0px;}
input[type="text"]{ color:#333 !important;}
textarea{color:#333 !important; border:1px solid #a0a0a0;}

/* hidden */
.hidden {display: block; position: absolute; overflow: hidden; top: -999999px; left: -999999px; width: 1px; height: 1px; font-size: 0; text-indent: -999999px; line-height: 0;}

/* button area 버튼 */
.button_area {font-size: 0px;}
.btn {display: inline-block; vertical-align: bottom; cursor:pointer; color: #f9f9f9; font-size: 14px; line-height: 35px; font-family: 'Noto Sans KR', sans-serif; font-weight: 400; border-radius: 7px; box-sizing: border-box;}
.btn_select {display: inline-block; vertical-align: bottom; cursor:pointer; color: #f9f9f9; font-size: 14px; line-height: 22px; font-family: 'Noto Sans KR', sans-serif; font-weight: 400; border-radius: 7px; box-sizing: border-box;}
.btn + .btn {margin-left: 10px;}
.btn img {border: 0; vertical-align: middle;}
.btn:hover {text-decoration: none;}
.btn.btn-s {height: 31px; line-height: 31px;}
.btn.btn-l {height: 42px; line-height: 42px;}
.btn.blue {background-color: #2a88e5;}
.btn.gray {background-color: #a1a1a1;}
.btn:hover.blue {background-color: #1e66ac;}
.btn:hover.gray {background-color: #807f7f;}

/* 스위치 버튼 */
.btn-switch {display: inline-block; text-align:center; vertical-align: middle; position: relative; width: 64px; height: 33px;}
.btn-switch input {display: none;}
.slide-object {position: absolute; cursor: pointer; top: 0; left: 0; right: 0; bottom: 0; text-align: center; background: #f3f3f3; z-index:10; border-radius: 20px;}
.slide-object:before {position: absolute; content: ""; height: 20px; width: 20px; left: 5px; bottom: 7px; background: #2a88e5; transition: 0.4s; -webkit-transition: 0.4s; -o-transition: 0.4s; -moz-transition: 0.4s; border-radius:50%; -webkit-border-radius:50%; -moz-border-radius:50%; box-shadow: 0 2px 2px #bfbebe; -moz-box-shadow: 0 2px 2px #bfbebe; -webkit-box-shadow: 0 2px 2px #bfbebe;}
input:checked + .slide-object:before {transform: translateX(34px); -webkit-transform: translateX(34px); -ms-transform: translateX(34px); -moz-transform: translateX(34px); -o-transform: translateX(34px); background: #d85050;}
.switch_txt {margin-left: 20px; color: #509dd8; font-size: 14px; font-weight: 600;  font-style: normal; line-height: 33px; transition: 0.4s;}
/* input & Select Type */
textarea {text-align: left; text-indent: 3px; line-height: 1.4em; border-radius: 5px; overflow:hidden;}
p {line-height: 1.5em; text-align: left;}

input[type=text]::-ms-clear {display: none;}
input {display: inline-block; display: inline; vertical-align: middle;}
input[type=text] {height: 42px; font-size: 100%; text-indent: 15px; line-height: 42px; text-align: left; border: 0px; font-family: 'Noto Sans KR R', sans-serif; background-color: #f2f2f2;}
input[type=password] {height: 42px; font-size: 100%; text-indent: 15px; line-height: 42px; text-align: left; border: 0px; font-family: 'Noto Sans KR R', sans-serif; background-color: #f2f2f2;}
input[type=submit] {font-family: 'Noto Sans KR';}
input[type=text].alignc {text-align: center;}

/* 셀렉트박스 */
.selectbox {position: relative; min-width: 100px; height: 42px; border: 0px; z-index: 1; background-color: #f2f2f2; text-align: left;}  
.selectbox::after {content: ""; position: absolute; top: 50%; right: 37px; width: 1px; height: 27px; transform: translate(0px, -50%); background-color: #bebebe;}
.selectbox:before {/* 화살표 대체 */content: ""; position: absolute; top: 50%; right: 15px; width: 0; height: 0; margin-top: -5px; border-left: 6px solid transparent; border-right: 6px solid transparent; border-top: 11px solid #565656;} 
.selectbox label {overflow: hidden; position: absolute; width: calc(100% - 60px); top: 50%; left: 5px; text-overflow: inherit; white-space: nowrap; transform: translate(10px, -50%); z-index: -1; /* IE8에서 label이 위치한 곳이 클릭되지 않는 것 해결 */ } 
.selectbox select {width: 100%; height: auto; padding-top: 23px; line-height: normal; font-family: inherit; border: 0; opacity: 0; /* 숨기기 */ filter:alpha(opacity=0); /* IE8 숨기기 */ -webkit-appearance: none; /* 네이티브 외형 감추기 */ -moz-appearance: none; appearance: none; }
.selectbox select option {font-size: initial;}

/* 체크박스 */
input[type=checkbox] {display: none;}
input[type=checkbox] + label{display: inline-block; cursor: pointer; position: relative; padding-left: 25px; margin-right: 15px; vertical-align: middle;}
input[type=checkbox] + label:before {position: absolute; display: inline-block; content: ""; width: 20px; height: 20px; margin-right: 10px; left: 0; bottom: 1px; background-color: #f2f2f2; border-radius: 3px; box-shadow: inset 1px 1px 1px 0px rgba(0, 0, 0, .3), 0px 1px 0px 0px rgba(255, 255, 255, .8);}
input[type=checkbox]:checked + label:before {content: "\2713";  /* 체크모양 */ text-shadow: 1px 1px 1px rgba(0, 0, 0, .2); font-size: 18px; font-weight:800; color: #fff; background:#848b90; text-align: center; line-height: 18px;} 

/* 라디오버튼 */
.checks {position: relative; display: inline-block; vertical-align: middle;}
.checks input[type="radio"] { position: absolute; width: 1px; height: 1px; padding: 0; margin: -1px; overflow: hidden; clip:rect(0,0,0,0); border: 0;}
.checks input[type="radio"] + label { display: inline-block; position: relative; padding-left: 30px; cursor: pointer; -webkit-user-select: none; -moz-user-select: none; -ms-user-select: none; }
.checks input[type="radio"] + label:before { content: ''; position: absolute; left: 0; top: 2px; width: 20px; height: 20px; text-align: center; background: #fafafa; border: 1px solid #cacece; border-radius: 100%; box-shadow: 0px 1px 2px rgba(0,0,0,0.05), inset 0px -15px 10px -12px rgba(0,0,0,0.05); box-sizing: border-box;}
.checks input[type="radio"] + label:active:before, .checks input[type="radio"]:checked + label:active:before { box-shadow: 0 1px 2px rgba(0,0,0,0.05), inset 0px 1px 3px rgba(0,0,0,0.1); }
.checks input[type="radio"]:checked + label:before { background: #E9ECEE; border-color: #adb8c0; }
.checks input[type="radio"]:checked + label:after { content: ''; position: absolute; top: 6px; left: 4px; width: 12px; height: 12px; background: #99a1a7; border-radius: 100%; box-shadow: inset 0px 0px 10px rgba(0,0,0,0.3); }


