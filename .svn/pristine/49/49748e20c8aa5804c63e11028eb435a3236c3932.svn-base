/* table list */
table.list {width: 100%; border-bottom: 2px solid #e6e8ea;}
  table.list.fixed {table-layout: fixed;}
  table.list thead th {padding: 20px 0px; font-size: 16px; color: #f9f9f9; text-align: center; vertical-align: middle; font-weight: 500; border-left: 1px solid #e3e3e3; background-color: #8f8f8f;}
  table.list thead th:first-child {border-left: 0px;}
  table.list thead th.alignl {padding-left: 20px; text-align: left;}
  table.list thead th.alignr {text-align: right;}
  table.list tbody th {border-top: 1px solid #e6e8ea; font-size: 14px; font-weight: 600; color: #000000; text-align: center;}
  table.list tbody td {padding: 15px 10px; border-top: 1px solid #e6e8ea; border-bottom: 0; border-right: 0; border-left: 1px solid #e3e3e3; font-size: 14px; color: #000000; text-align: center; line-height: 1.4em; vertical-align: middle; word-break: break-all;}
  table.list tbody td:first-child {border-left: 0px;}
  table.list tbody th.alignl,
  table.list tbody td.alignl {padding-left: 20px; padding-right: 20px; text-align: left;}
  table.list tbody th.alignr,
  table.list tbody td.alignr {text-align: right;}
  table.list tbody td.ellipsis_txt {table-layout: fixed; overflow: hidden; color: #333333; text-align: left; text-overflow: ellipsis; white-space: nowrap;}
  table.list tbody td a {color: #626262; text-decoration: none; vertical-align: middle;}
  table.list tbody td a:hover {text-decoration: underline;}
  table.list tbody tr td.nopadding {padding: 0px;}
  table.list tbody tr td img {display: inline; display: inline-block; vertical-align: middle;}
 
/* table search */
table.search {width: 100%;}
  table.search.fixed {table-layout: fixed;}
  table.search tbody th {font-size: 16px; font-weight: 500; color: #000000; text-align: right;}
  table.search tbody td {padding: 5px 7px; font-size: 14px; color: #000000; text-align: center; line-height: 1.4em; vertical-align: middle; word-break: break-all;}
  table.search tbody td:first-child {border-left: 0px;}
  table.search tbody td:last-child {padding-right: 0px;}
  
/* table write */
table.write {width: 100%; border-top: 1px solid #bfbfbf; border-bottom: 1px solid #bfbfbf;}
table.write.fixed {table-layout: fixed;}
table.write tbody th {padding: 15px 30px; font-size: 16px; font-weight: bold; color: #fff; text-align: right; border-top: 1px solid #bfbfbf; background-color: #8f8f8f;}
table.write tbody td {padding: 5px 7px; font-size: 16px; color: #000000; text-align: left; line-height: 1.4em; vertical-align: middle; word-break: break-all; border-top: 1px solid #bfbfbf;}
table.write tbody th span.impo {position: relative;}
table.write tbody th span.impo::before {display: inline-block; margin-right: 5px; content: "*"; color: #de9643;}

/* 페이징 */
.pagination_area {overflow: hidden;}
  .pagination_area ul {overflow: hidden; text-align: center;}
  .pagination_area ul li {display: inline-block; margin-right: 15px; margin-left: -4px; vertical-align: middle;}
  .pagination_area ul li:first-child {margin-left: 0px; margin-right: 10px;}
  .pagination_area ul li:last-child {margin-right: 0px;}
  .pagination_area ul li:nth-last-child(2) {margin-right: 10px;}
  .pagination_area ul li a {font-size: 14px; color: #565656; vertical-align: middle;}
  .pagination_area ul li a img {margin-top: 2px;}
  .pagination_area ul li a:hover {color: #5291cf; text-decoration: underline;}
  .pagination_area ul li.on a {color: #5291cf;}

/* 텝메뉴 */
.tab_area {overflow: hidden;}
.tab_area ul {padding: 0 23px; border-bottom: 2px solid #d8d8d8;}
.tab_area ul li {display: inline-block; min-width: 165px; height: 40px; line-height: 40px; border: 2px solid #d8d8d8; border-bottom: 0px; border-radius: 10px 10px 0 0; background-color: #d8d8d8;}
.tab_area ul li a {display: block; color: #f9f9f9; font-size: 16px; font-weight: bold; text-align: center;}
.tab_area ul li.on {position: relative; background-color: #fff;}
.tab_area ul li.on a {color: #000;}
.tab_area ul li.on::after {position: absolute; display: block; width: 100%; height: 2px; bottom: -2px; left: 0px; content: ""; background-color: #fff;}



