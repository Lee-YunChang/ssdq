			$('#insttCode').css('width','70%');
		}
		
		$("#guide_txt").text(guide_txt_1);
		$("#sub_txt").text(sub_txt_1);
		$("#insttNm, #insttCode").css({'cursor':'default'});
	});
	$(".btn_basic_find").click(function(){
		$("#grid").empty();
		$("div[id=pagination]").empty();
		fn_insttFindLayer();;
	});
	$(".btn_close").click(function(){
		$("#grid").empty();
		$("div[id=pagination]").empty();
		$("div[id=wholegrid]").removeAttr("style");
		fn_modalCloseLayer();
	});
	$(".btn_basic_next").click(function() {
		$('body').removeClass().addClass('bg03');
		if(!$("#basic li").last().is(":visible")){
			var nextNo = $("#basic li:visible").next("li").attr("data-liNo");
			var insttCode = $("#insttCode").val();
			var insttNm = $("#insttNm").val();
			
			if(insttCode == "" || insttNm == ""){
				if("N" == pubInstYn){
					alert(UIT0252);
				}else{
					alert(UIT0032);
				}
				return false;
			}
			
			$("#selectInsttCode").val(insttCode);
			$("#selectInsttNm").val(insttNm);
			
			// 문구
			fn_text(nextNo);
			$("#DB_text").css('display','block');
			
			//DB 생성 완료시 수정 불가
			$("#insttNm").attr("readonly",true);
			$('.btn_basic_next').hide();
			
			// 가이드 바 
			//$(".state_li").attr("class", "");
			//$("#state_"+nextNo).attr("class", "state_li on");
			
			// 버튼
			//$("#basic li:visible").hide().next("li").fadeIn("80");
			//$(".btn_basic_prev").removeClass("off");
			$(".btn_basic_prev").show();
			$(".btn_basic_end").show();
			
			//관리자 정보 show
			$(".db_right_area").show();
			
		}
		
		//if($("#basic li").last().is(":visible")){
			//$(this).addClass("off");
			//$(this).hide();
			//$(".btn_basic_end").show();
		//}
		return false;
	});
	
	
	$(".btn_basic_prev" ).click(function() {
		$('body').removeClass().addClass('bg04');
		
		var prevNo = $("#basic li:visible").prev("li").attr("data-liNo");
		
		// 문구
		fn_text(prevNo);
		
		// 가이드 바 
		//$(".state_li").attr("class", "");
		//$("#state_"+prevNo).attr("class", "state_li on");
		
		//관리자 정보 hide 초기화
		$(".db_right_area").hide();
		$("#userNm").val("");
		$("#userId").val("");
		$("#userPw").val("");
		$("#userRePw").val("");
		
		// 버튼 
		if(!$("#basic li").first().is(":visible")){
			$("#basic li:visible").hide().prev("li").fadeIn("80");
			//$(".btn_basic_next").removeClass("off");
			$(".btn_basic_next").show();
			$(".btn_basic_prev").hide();
			$(".btn_basic_end").hide();
			$("#DB_text").css('display','none');
			$("#insttNm").attr("readonly",false);
			
		}
		
		if($("#basic li").first().is(":visible")){
			//$(this).addClass("off");
			$(this).hide();
			$(".btn_basic_end").hide();
		}
		return false;
	});
	
	
	$(".btn_basic_end").click(function() {
		
		//var thisNo = $("#basic li:visible").attr("data-liNo");
		var frmChk = false;
		
		//if(thisNo == 2){
			
			var chk = $("#submitChk").val();
